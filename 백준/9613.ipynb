    "def gcd(x,y):\n",
    "    if y==0: return x\n",
    "    return gcd(y,x%y)\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    a=[]\n",
    "    d=list(map(int,input().split()))\n",
    "    for i in range(1,len(d)-1):\n",
    "        for j in range(i+1,len(d)):\n",
    "            tmp=gcd(d[i],d[j])\n",
    "            a.append(tmp)\n",
    "    print(sum(a))"
   ]
