    "n=int(input())\n",
    "for i in range(1, n+1):\n",
    "    print(\" \"*(n-i)+\"* \"*(i))\n",
    "    \"\"\"\n",
    "    if i % 2 ==0:\n",
    "        for j in range(i):\n",
    "            print(\"* \",end=\"\")\n",
    "    else:\n",
    "        for j in range(i-1):\n",
    "            print(\"* \",end=\"\")\"\"\""
