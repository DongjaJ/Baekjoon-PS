    "n=int(input())\n",
    "d=[[1 for i in range(10)] for j in range(1001)]\n",
    "for i in range(2,n+1):\n",
    "    for j in range(10):\n",
    "        if j == 0:\n",
    "            d[i][j]=d[i-1][j]%10007\n",
    "        else:\n",
    "            d[i][j]=sum(d[i-1][:j+1])%10007\n",
    "print(sum(d[n])%10007)"
   ]
