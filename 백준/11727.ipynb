    "n = int(input())\n",
    "if n==1:\n",
    "    print(1)\n",
    "elif n==2:\n",
    "    print(3)\n",
    "else:\n",
    "    d=[0]*(n+1)\n",
    "    d[1],d[2]=1,3\n",
    "    for i in range(3,n+1):\n",
    "        d[i]=d[i-1]+d[i-2]*2\n",
    "    print(d[i]%10007)"
   ]
