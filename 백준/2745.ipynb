    "n,b=input().split()\n",
    "ans,k,b=0,1,int(b)\n",
    "\n",
    "for i in reversed(n):\n",
    "    if i.isdigit()==1: ans+=int(i)*k\n",
    "    else:\n",
    "        tmp =ord(i)-55\n",
    "        ans+=tmp*k\n",
    "    k*=b\n",
    "print(ans)"
