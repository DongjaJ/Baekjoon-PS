   "source": [
    "n=int(input())\n",
    "a=[]\n",
    "if n==0: print(0)\n",
    "while n:\n",
    "    if n%(-2):\n",
    "        a.append(n%(-2))\n",
    "        n=n//-2+1\n",
    "    else:\n",
    "        a.append(n%-2)\n",
    "        n=n//-2\n",
    "a.reverse()\n",
    "for i in a:\n",
    "    print(i,end='') if i > 0 else print(-i,end='')"
   ]
