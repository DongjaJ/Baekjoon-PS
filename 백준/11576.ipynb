   "source": [
    "a,b=map(int,input().split())\n",
    "input()\n",
    "n=list(map(int,input().split()))\n",
    "tmp,k=0,1\n",
    "for i in reversed(n):\n",
    "    tmp+=i*k\n",
    "    k*=a\n",
    "ans=[]\n",
    "while tmp:\n",
    "    ans.append(tmp%b)\n",
    "    tmp//=b\n",
    "ans.reverse()\n",
    "for i in ans:\n",
    "    print(i,end=' ')"
   ]
