    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n=int(input())\n",
    "    if n <= 2:\n",
    "        print(n)\n",
    "    elif n==3:\n",
    "        print(4)\n",
    "    else:\n",
    "        d=[0]*(n+1)\n",
    "        d[1],d[2],d[3]=1,2,4\n",
    "        for i in range(4,n+1):\n",
    "            d[i]=d[i-1]+d[i-2]+d[i-3]\n",
    "        print(d[n])"
