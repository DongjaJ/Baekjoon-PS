    "n=int(input())\n",
    "d=[i for i in range(n+1)]\n",
    "for i in range(1,n+1):\n",
    "    for j in range(1,i):\n",
    "        if j*j >i:\n",
    "            break\n",
    "        d[i]=min(d[i],d[i-j*j]+1)\n",
    "print(d[n])"
   ]
