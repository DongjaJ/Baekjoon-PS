    "n=list(input())\n",
    "ans=[]\n",
    "n.reverse()\n",
    "k,tmp=1,0\n",
    "if len(n)==1: print(int(n[0]))\n",
    "for i in range(len(n)):\n",
    "    tmp+=int(n[i])*k\n",
    "    k*=2\n",
    "    if i!=0 and (i%3==2 or i==len(n)-1):\n",
    "        ans.append(tmp)\n",
    "        tmp,k=0,1\n",
    "    \n",
    "for i in reversed(ans):\n",
    "    print(i,end='')\n",
    "        \n",
    "    "
