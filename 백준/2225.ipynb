    "n,k=map(int,input().split())\n",
    "d=[[0]*201 for i in range(201)]\n",
    "for i in range(201):\n",
    "    d[0][i]=1\n",
    "for i in range(1,n+1):\n",
    "    for j in range(1,k+1):\n",
    "            d[i][j]=d[i-1][j]%1000000000+d[i][j-1]%1000000000\n",
    "print(d[n][k]%1000000000)"
   ]
