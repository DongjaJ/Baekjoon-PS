    "n=int(input())\n",
    "#첫째행 길이, 둘째행 합\n",
    "d=[0]*1001\n",
    "A=list(map(int,input().split()))\n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if A[i] > A[j]:\n",
    "            d[i]=max(d[i],d[j]+A[i])\n",
    "    if d[i] == 0:\n",
    "        d[i]=A[i]\n",
    "print(max(d))"
   ]
